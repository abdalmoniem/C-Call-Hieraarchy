{
    "name": "c-call-hierarchy",
    "publisher": "AbdAlMoniem-AlHifnawy",
    "displayName": "C Call Hierarchy",
    "description": "Build C Call Hierarchy using cscope",
    "version": "1.5.0",
    "engines": {
        "vscode": "^1.52.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/abdalmoniem/C-Call-Hierarchy.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "C",
        "Call Graph",
        "Call Hierarchy"
    ],
    "icon": "extension_icon.png",
    "activationEvents": [
        "onLanguage:c"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "cCallHierarchy.build",
                "title": "Build Database",
                "icon": "$(extensions-view-icon)"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    },
    "dependencies": {
        "vsce": "^2.7.0"
    }
}