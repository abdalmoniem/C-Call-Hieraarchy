{
    "name": "cCallHierarchy",
    "publisher": "AbdAlMoniem AlHifnawy",
    "displayName": "cCallHierarchy",
    "description": "Build Call Hierarchy Based on cscope",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "C",
        "Call Graph",
        "Hierarchy"
    ],
    "icon": "extension_icon.png",
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "cCallHierarchy.build",
                "title": "Build Database",
                "icon": "$(extensions-view-icon)"
            },
            {
                "command": "cCallHierarchy.findcaller",
                "title": "Show Call Hierarchy",
                "icon": "$(symbol-reference)"
            },
            {
                "command": "cCallHierarchy.gotodef",
                "title": "Goto Def",
                "icon": "$(notebook-open-as-text)"
            },
            {
                "command": "cCallHierarchy.gotoline",
                "title": "Goto Line",
                "icon": "$(preferences-open-settings)"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "cCallHierarchy",
                    "title": "C Call Hierarchy",
                    "icon": "$(references)"
                }
            ]
        },
        "views": {
            "cCallHierarchy": [
                {
                    "id": "cHierarchyView",
                    "name": "C Call Hierarchy",
                    "contextualTitle": "Tree Explorer",
                    "type": "tree"
                }
            ]
        },
        "menus": {
            "view/item/context": [
                {
                    "command": "cCallHierarchy.gotodef",
                    "when": "view == cHierarchyView",
                    "group": "inline"
                },
                {
                    "command": "cCallHierarchy.gotoline",
                    "when": "view == cHierarchyView",
                    "group": "inline"
                }
            ],
            "editor/context": [
                {
                    "command": "cCallHierarchy.build",
                    "when": "editorTextFocus && resourceLangId == c",
                    "group": "inline"
                },
                {
                    "command": "cCallHierarchy.findcaller",
                    "when": "editorTextFocus && resourceLangId == c",
                    "group": "inline"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    },
    "dependencies": {
        "vsce": "^2.7.0"
    }
}